{
  "rules": {
    "class-name": true,
    "curly": true,
    "eofline": false,
    "forin": true,
    "indent": [true, 4],
    "interface-name" : false,
    "label-position": true,
    "max-line-length": [true, 300],
    "no-arg": true,
    "no-bitwise": false,
    "no-console": [true,
        "debug",
        "info",
        "time",
        "timeEnd",
        "trace",
        "log"
    ],
    "no-construct": true,
    "no-debugger": true,
    "no-duplicate-variable": true,
    "no-empty": false,
    "no-eval": true,
    "no-string-literal": false,
    "no-trailing-whitespace": false,
    "no-unused-expression": false,
    "no-unused-variable":false,
    "no-use-before-declare": true,
    "one-line": [true,
        "check-open-brace",
        "check-catch",
        "check-else",
        "check-whitespace"
    ],
    "quotemark": [false, "double"],
    "radix": true,
    "semicolon": true,
    "triple-equals": [true, "allow-null-check"],
    "typedef": [true,
        "propertySignature",
        "memberVeriableDeclarator"
    ],
    "variable-name": false,
    "whitespace": [true,
        "check-branch",
        "check-decl",
        "check-operator",
        "check-separator",
        "check-type"
    ]
  }
}
